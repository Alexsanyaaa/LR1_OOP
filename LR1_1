            Console.WriteLine("Увлечений, хобби, интересы: футбол, судоку, произведения Сапковского, Харриса и Конан Дойля");

            double g = 9.98;
            double h = 3.5f;
            int d17 = 17;

            double ee = g * h / (double)d17 + d17 / h - (g + d17 - h) / 4;
            Console.WriteLine($"ee = {ee}");

            Console.ReadKey();
        }
    }
